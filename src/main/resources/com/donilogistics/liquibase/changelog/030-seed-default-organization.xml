<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.20.xsd">

    <changeSet id="030-seed-doni-organization" author="doni">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">SELECT COUNT(*) FROM ORGANIZATION WHERE CODE = 'DONI'</sqlCheck>
        </preConditions>
        <insert tableName="ORGANIZATION">
            <column name="ID" value="11111111-1111-1111-1111-111111111111"/>
            <column name="VERSION" valueNumeric="1"/>
            <column name="NAME" value="Doni Logistics"/>
            <column name="CODE" value="DONI"/>
            <column name="DESCRIPTION" value="Default organization for Super Admin"/>
            <column name="ADDRESS" value=""/>
            <column name="PHONE_NUMBER" value=""/>
            <column name="EMAIL" value="info@doni.local"/>
            <column name="WEBSITE" value="https://doni.local"/>
            <column name="LOGO" value=""/>
            <column name="ACTIVE" valueBoolean="true"/>
        </insert>
    </changeSet>

</databaseChangeLog>


